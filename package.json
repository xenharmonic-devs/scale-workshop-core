  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^20.10.1",
    "gts": "^5.2.0",
    "peggy": "^3.0.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "scripts": {
    "lint": "gts lint",
